{
  "mcpServers" : {
    "calculator" : {
      "args" : [ ],
      "domain" : "math",
      "description" : "Calculadora matemática",
      "env" : {
        "REQUIRES_PYTHON" : "true"
      },
      "priority" : 1,
      "command" : "uvx calculator-mcp-server",
      "enabled" : true
    },
    "web-fetch" : {
      "args" : [ ],
      "domain" : "internet",
      "description" : "Busca web oficial sem browser",
      "env" : {
        "REQUIRES_ONLINE" : "true",
        "REQUIRES_PYTHON" : "true"
      },
      "priority" : 2,
      "command" : "uvx mcp-server-fetch",
      "enabled" : true
    },
    "time" : {
      "args" : [ ],
      "domain" : "time",
      "description" : "Servidor para ferramentas de tempo e fuso horário",
      "env" : {
        "REQUIRES_UVX" : "true"
      },
      "priority" : 1,
      "command" : "uvx mcp-server-time",
      "enabled" : true
    },
    "weather-nws" : {
      "args" : [ ],
      "domain" : "weather",
      "description" : "Previsões meteorológicas via NWS",
      "env" : {
        "REQUIRES_NODEJS" : "true",
        "REQUIRES_ONLINE" : "true"
      },
      "priority" : 1,
      "command" : "npx @h1deya/mcp-server-weather",
      "enabled" : true
    },
    "filesystem" : {
      "args" : [ ],
      "domain" : "filesystem",
      "description" : "Sistema de arquivos - Documents",
      "env" : {
        "REQUIRES_NODEJS" : "true"
      },
      "priority" : 3,
      "command" : "npx -y @modelcontextprotocol/server-filesystem ./documents",
      "enabled" : true
    },
    "rss-feeds" : {
      "args" : [ ],
      "domain" : "internet",
      "description" : "Feeds RSS básicos",
      "env" : {
        "REQUIRES_ONLINE" : "true",
        "REQUIRES_PYTHON" : "true"
      },
      "priority" : 1,
      "command" : "uvx mcp-server-rss",
      "enabled" : true
    }
  }
}